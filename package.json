    "scripts": {
        "analyse": "webpack --config=./webpack-dist.config --stats=detailed --json=stats.json",
        "build": "webpack --config=./webpack-dist.config",
        "build:dev": "webpack --config=./webpack.config",
        "cc:reportAll": "scripts/code-coverage.sh",
        "clean": "rm -rf assets coverage **/coverage cypress/screenshots cypress/videos dist node_modules **/node_modules stats.json .nyc_output",
        "codestyles:files": "cross-env prettier-eslint './*.js' --list-different -l silent",
        "codestyles:fix": "cross-env prettier-eslint --write",
        "codestyles:fix:all": "cross-env prettier-eslint '**/*.js' --write",
        "codestyles:fix:staged": "cross-env FILES=$(git diff --cached --name-only --diff-filter=ACM | grep \".jsx\\{0,1\\}$\"); npx prettier-eslint $FILES --write",
        "cypress:clean": "cypress cache clear",
        "cypress:dashboard": "cypress run --record --parallel --config video=true --ci-build-id $CI_BUILD_NUMBER",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress:watch": "cypress run --headed",
        "eslint": "eslint '**/*.js' --quiet",
        "eslint:fix": "eslint '**/*.js' --quiet --fix",
        "start": "webpack-dev-server",
        "start:build": "cross-env NODE_ENV=production URL=dev-espace.library.uq.edu.au NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server --config=./webpack-dist.config --disable-host-check",
        "start:build:e2e": "cross-env NODE_ENV=production URL=localhost USE_MOCK=true NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server --config=./webpack-dist.config",
        "start:mock": "cross-env USE_MOCK=true FULL_PATH=http://localhost:3000 NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
        "start:url": "cross-env URL=dev-espace.library.uq.edu.au NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
        "test": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest --maxWorkers=8",
        "test:all": "npm run test:unit && npm run test:e2e && npm run test:cs -s",
        "test:cs": "echo \"Running code style checks...\" && FILES=$(npm run codestyles:files -s) && echo \"No code style issues found.\" || echo \"File(s) failed code style checks!:\" && echo $FILES",
        "pretest:e2e": "rm -rf .nyc_output",
        "test:e2e": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'cypress run --record false'",
        "pretest:e2e:cc": "rm -rf .nyc_output coverage/cypress",
        "test:e2e:cc": "cross-env NODE_ENV=cc npm run test:e2e",
        "posttest:e2e:cc": "rm -rf .nyc_output",
        "test:e2e:dashboard": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'npm run cypress:dashboard'",
        "test:e2e:open": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'npm run cypress:open'",
        "test:e2e:watch": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'npm run cypress:watch'",
        "test:unit": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest",
        "test:unit:serial": "npm run test:unit -- --ci --runInBand",
        "test:unit:serial:skipcoverage": "npm run test:unit -- --ci --runInBand --no-coverage",
        "test:unit:ci1": "npm run test:unit -- --ci --testPathIgnorePatterns=SharedComponents --coveragePathIgnorePatterns=SharedComponents",
        "test:unit:ci2": "npm run test:unit -- --ci SharedComponents --collectCoverageFrom=\\'[\\\"**/SharedComponents/**\\\",\\\"!**/containers/**\\\",\\\"!**/index.js\\\"]\\'",
        "test:unit:ci1:skipcoverage": "npm run test:unit -- --ci --no-coverage --testPathIgnorePatterns=SharedComponents",
        "test:unit:ci2:skipcoverage": "npm run test:unit -- --ci --no-coverage SharedComponents",
        "test:unit:update": "npm run test:unit -- --updateSnapshot",
        "test:unit:watch": "npm run test:unit -- --watch --onlyChanged"
    },
    "engines": {
        "npm": "^8.4.1",
        "node": "^16.13.2"
    },
    "babel": {
        "presets": [
            "@babel/env",
            "@babel/react"
        ],
        "env": {
            "development": {
                "presets": [
                    "@babel/env",
                    "@babel/react"
                ]
            },
            "production": {
                "presets": [
                    [
                        "@babel/env",
                        {
                            "es2015": {
                                "modules": false
                            },
                            "targets": {
                                "ie": 11,
                                "terser": true
                            }
                        }
                    ]
                ],
                "plugins": [
                    "@babel/plugin-transform-react-constant-elements",
                    "transform-react-remove-prop-types",
                    "@babel/plugin-proposal-class-properties",
                    "@babel/plugin-proposal-export-namespace-from",
                    "@babel/plugin-proposal-export-default-from",
                    "@babel/plugin-proposal-class-properties",
                    "@babel/plugin-proposal-optional-chaining",
                    [
                        "@babel/plugin-transform-spread",
                        {
                            "loose": true
                        }
                    ]
                ]
            },
            "test": {
                "presets": [
                    "@babel/env",
                    "@babel/react"
                ],
                "plugins": [
                    [
                        "dynamic-import-node",
                        {
                            "noInterop": true
                        }
                    ],
                    [
                        "@babel/plugin-transform-runtime",
                        {
                            "helpers": false,
                            "regenerator": true,
                            "corejs": false
                        }
                    ],
                    "@babel/plugin-proposal-export-namespace-from",
                    "@babel/plugin-proposal-export-default-from",
                    "@babel/plugin-proposal-class-properties",
                    "@babel/plugin-proposal-optional-chaining"
                ]
            },
            "cc": {
                "presets": [
                    "@babel/env",
                    "@babel/react"
                ],
                "plugins": [
                    [
                        "dynamic-import-node",
                        {
                            "noInterop": true
                        }
                    ],
                    [
                        "@babel/plugin-transform-runtime",
                        {
                            "helpers": false,
                            "regenerator": true,
                            "corejs": false
                        }
                    ],
                    "@babel/plugin-proposal-export-namespace-from",
                    "@babel/plugin-proposal-export-default-from",
                    "@babel/plugin-proposal-class-properties",
                    "@babel/plugin-proposal-optional-chaining",
                    "istanbul"
                ]
            }
        }
    },
    "jest": {
        "transform": {
            ".*": "<rootDir>/node_modules/babel-jest"
        },
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(test).[jt]s?(x)",
            "**/?(*.)+(integration).[jt]s?(x)"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "es6"
        ],
        "unmockedModulePathPatterns": [
            "react",
            "enzyme"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/modules/**/*.{js,jsx}",
            "!src/modules/App/components/pages.js",
            "!src/**/{index,locale,config}.js",
            "!src/**/containers/*.{js,jsx}",
            "src/modules/Index/*.{js,jsx}",
            "src/actions/*.{js,jsx}",
            "src/helpers/*.{js,jsx}",
            "src/middleware/*.{js,jsx}",
            "src/reducers/*.{js,jsx}",
            "src/repositories/routes.js"
        ],
        "coverageDirectory": "coverage/jest",
        "coverageReporters": [
            "html",
            "text",
            "json"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 100,
                "branches": 100,
                "functions": 100,
                "lines": 100
            }
        },
        "modulePaths": [
            "<rootDir>/src",
            "<rootDir>/eslint-plugin-uqlibrary",
            "<rootDir>/utils"
        ],
        "moduleNameMapper": {
            "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styles.js",
            "\\.(jpg|jpeg|svg)$": "<rootDir>/__mocks__/images.js"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "setupFiles": [
            "./src/test.setup.js",
            "jest-localstorage-mock"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/jest.setup.js"
        ],
        "testURL": "http://fez-staging.library.uq.edu.au"
    },
    "nyc": {
        "reporter": [
            "html",
            "text-summary",
            "json"
        ],
        "report-dir": "coverage/cypress",
        "temp-dir": "coverage/temp",
        "include": [
            "src/modules/**/*.{js,jsx}",
            "src/modules/Index/*.{js,jsx}",
            "src/actions/*.{js,jsx}",
            "src/helpers/*.{js,jsx}",
            "src/middleware/*.{js,jsx}",
            "src/reducers/*.{js,jsx}",
            "src/repositories/routes.js"
        ],
        "exclude": [
            "src/**/{index,locale,config}.js",
            "src/**/containers/*.{js,jsx}"
        ],
        "all": true
    }
}
