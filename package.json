  "scripts": {
    "jest-preview": "jest-preview",
    "analyse": "webpack --config=./webpack-dist.config --stats=detailed --json=stats.json",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' webpack --config=./webpack-dist.config",
    "build:dev": "NODE_OPTIONS='--max-old-space-size=8192' webpack --config=./webpack.config",
    "cc:reportAll": "scripts/code-coverage.sh",
    "clean": "rm -rf assets coverage **/coverage cypress/screenshots cypress/videos dist node_modules **/node_modules stats.json .nyc_output",
    "codestyles:files": "cross-env prettier-eslint './*.js' --list-different -l silent",
    "codestyles:fix": "cross-env prettier-eslint --write",
    "codestyles:fix:all": "cross-env prettier-eslint '**/*.{js,jsx,ts,tsx}' --write",
    "codestyles:fix:staged": "cross-env FILES=$(git diff --cached --name-only --diff-filter=ACM | grep \".jsx\\{0,1\\}$\"); npx prettier-eslint $FILES --write",
    "cypress:clean": "cypress cache clear",
    "cypress:dashboard": "cypress run --record --parallel --config video=true --ci-build-id $CI_BUILD_NUMBER",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:watch": "cypress run --headed",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}' --quiet",
    "eslint:fix": "eslint '**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "start": "webpack-dev-server",
    "start:build": "cross-env NODE_ENV=production URL=dev-espace.library.uq.edu.au NODE_OPTIONS='--max-old-space-size=8192' webpack-dev-server --config=./webpack-dist.config --allowed-hosts=all",
    "start:build:e2e": "cross-env NODE_ENV=production URL=localhost USE_MOCK=true NODE_OPTIONS='--max-old-space-size=8192' webpack-dev-server --config=./webpack-dist.config",
    "start:mock": "cross-env USE_MOCK=true FULL_PATH=http://localhost:3000 NODE_OPTIONS='--max-old-space-size=4096' webpack-dev-server",
    "start:url": "cross-env URL=dev-espace.library.uq.edu.au NODE_OPTIONS='--max-old-space-size=4096' webpack-dev-server",
    "test": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest --maxWorkers=8",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:cs -s",
    "pretest:cc": "rm -rf coverage .nyc_output",
    "test:cc": "npm run test:unit:ci && npm run test:e2e:cc && npm run cc:reportAll",
    "test:cs": "echo \"Running code style checks...\" && FILES=$(npm run codestyles:files -s) && echo \"No code style issues found.\" || echo \"File(s) failed code style checks!:\" && echo $FILES",
    "pretest:e2e": "rm -rf .nyc_output",
    "test:e2e": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'cypress run --record false'",
    "pretest:e2e:cc": "rm -rf .nyc_output coverage/cypress",
    "test:e2e:cc": "cross-env NODE_ENV=cc npm run test:e2e",
    "test:e2e:ci": "cross-env NODE_ENV=cc start-server-and-test 'npm run start:mock' http-get://localhost:3000",
    "test:e2e:ci1": "npm run test:e2e:ci 'cypress run --record false --spec $(paste -sd, bin/group1.txt)'",
    "test:e2e:ci2": "npm run test:e2e:ci 'cypress run --record false --spec $(paste -sd, bin/group2.txt)'",
    "test:e2e:ci1:dashboard": "npm run test:e2e:ci 'cypress run --spec $(paste -sd, bin/group1.txt) --record --parallel --config video=true --group group-1 --ci-build-id $CI_BUILD_NUMBER'",
    "test:e2e:ci2:dashboard": "npm run test:e2e:ci 'cypress run --spec $(paste -sd, bin/group2.txt) --record --parallel --config video=true --group group-2 --ci-build-id $CI_BUILD_NUMBER'",
    "posttest:e2e:cc": "rm -rf .nyc_output",
    "test:e2e:dashboard": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'npm run cypress:dashboard'",
    "test:e2e:open": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'npm run cypress:open'",
    "test:e2e:watch": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'npm run cypress:watch'",
    "test:unit": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest",
    "test:unit:serial": "npm run test:unit -- --runInBand",
    "pretest:unit:ci": "rm -rf coverage",
    "test:unit:ci": "npm run test:unit -- --ci --silent --no-warnings --testPathIgnorePatterns=$(paste -sd'|' jest-serial.txt | sed 's/[.[\\]*^$/\\\\&/g')",
    "test:unit:ci:nocoverage": "npm run test:unit:ci -- --no-coverage",
    "test:unit:ci1:nocoverage": "npm run test:unit:ci:nocoverage -- --testPathIgnorePatterns=SharedComponents",
    "test:unit:ci2:nocoverage": "npm run test:unit:ci:nocoverage -- SharedComponents",
    "test:unit:ci:serial": "npm run test:unit:serial -- --ci --silent --no-warnings $(paste -sd' ', jest-serial.txt)",
    "test:unit:ci:serial:nocoverage": "npm run test:unit:ci:serial -- --no-coverage",
    "test:unit:update": "npm run test:unit -- --updateSnapshot",
    "test:unit:watch": "npm run test:unit -- --watch --onlyChanged",
    "tsc": "./node_modules/typescript/bin/tsc",
    "tsc:watch": "./node_modules/typescript/bin/tsc --watch"
  },
  "engines": {
    "npm": ">=10.2.3",
    "node": ">=18.19.0"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/preset-typescript"
    ],
    "env": {
      "development": {
        "presets": [
          "@babel/env",
          "@babel/react",
          "@babel/preset-typescript"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/env",
            {
              "modules": false,
              "targets": {
                "ie": 11
              }
            }
          ]
        ],
        "plugins": [
          "@babel/plugin-transform-react-constant-elements",
          "transform-react-remove-prop-types",
          "@babel/plugin-proposal-export-default-from",
          [
            "@babel/plugin-transform-spread",
            {
              "loose": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          "@babel/env",
          "@babel/react",
          "@babel/preset-typescript"
        ],
        "plugins": [
          [
            "dynamic-import-node",
            {
              "noInterop": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": false,
              "regenerator": true,
              "corejs": false
            }
          ],
          "@babel/plugin-proposal-export-default-from"
        ]
      },
      "cc": {
        "presets": [
          "@babel/env",
          "@babel/react",
          "@babel/preset-typescript"
        ],
        "plugins": [
          [
            "dynamic-import-node",
            {
              "noInterop": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": false,
              "regenerator": true,
              "corejs": false
            }
          ],
          "@babel/plugin-proposal-export-default-from",
          "istanbul"
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(test).[jt]s?(x)",
      "**/?(*.)+(integration).[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "tsx",
      "ts",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/modules/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/modules/App/components/pages.js",
      "!src/**/{index,locale,config}.js",
      "!src/**/containers/*.{js,jsx,ts,tsx}",
      "!src/**/test-utils.{ts,js}",
      "src/modules/Index/*.{js,jsx,ts,tsx}",
      "src/actions/*.{js,jsx,ts,tsx}",
      "src/helpers/*.{js,jsx,ts,tsx}",
      "src/middleware/*.{js,jsx,ts,tsx}",
      "src/reducers/*.{js,jsx,ts,tsx}",
      "src/repositories/routes.js",
      "src/hooks/useForm.js",
      "src/hooks/useValidatedForm.js"
    ],
    "coverageDirectory": "coverage/jest",
    "coverageReporters": [
      "cobertura",
      "html",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/eslint-plugin-uqlibrary",
      "<rootDir>/utils"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styles.js",
      "\\.(jpg|jpeg|svg)$": "<rootDir>/__mocks__/images.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "reporters": [
      "default",
      "jest-summary-reporter",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report",
          "outputPath": "coverage/jest-html-report.html",
          "includeFailureMsg": true,
          "includeConsoleLog": true,
          "executionTimeWarningThreshold": 30,
          "statusIgnoreFilter": "passed"
        }
      ]
    ],
    "setupFiles": [
      "./src/globals.setup.js",
      "./src/test.setup.js",
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://fez-staging.library.uq.edu.au"
    },
    "testTimeout": 30000,
    "snapshotSerializers": [
      "<rootDir>/utils/jest-serializer.ts"
    ]
  },
  "nyc": {
    "reporter": [
      "html",
      "text-summary",
      "json"
    ],
    "report-dir": "coverage/cypress",
    "temp-dir": "coverage/temp",
    "include": [
      "src/modules/**/*.{js,jsx,ts,tsx}",
      "src/modules/Index/*.{js,jsx,ts,tsx}",
      "src/actions/*.{js,jsx,ts,tsx}",
      "src/helpers/*.{js,jsx,ts,tsx}",
      "src/middleware/*.{js,jsx,ts,tsx}",
      "src/reducers/*.{js,jsx,ts,tsx}",
      "src/repositories/routes.js"
    ],
    "exclude": [
      "src/**/{index,locale,config,pages}.js",
      "src/**/containers/*.{js,jsx,ts,tsx}",
      "src/**/*.test.{js,jsx,ts,tsx}",
      "!src/modules/Admin/Doi/components/**",
      "!src/modules/Admin/FavouriteSearch/components/**",
      "!src/modules/Admin/ManageAuthors/**",
      "!src/modules/Admin/ManageAuthors/partials/**",
      "!src/modules/Admin/ManageUsers/**",
      "!src/modules/Admin/ManageUsers/partials/**",
      "!src/modules/Admin/MasterJournalListIngest/components/**",
      "!src/modules/AuthorIdentifiers/components/**",
      "!src/modules/BulkUpdates/**",
      "!src/modules/MyEditorialAppointments/**",
      "!src/modules/SharedComponents/Toolbox/ScrollToSection/components/**",
      "!src/modules/SharedComponents/Toolbox/TabbedCard/components/**",
      "!src/modules/ThirdPartyLookupTool/components/**",
      "!src/modules/ViewJournal/components/**",
      "!src/modules/ViewJournal/components/partials/",
      "src/reducers/account.ts"
    ],
    "all": true
  }
}