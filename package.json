    "scripts": {
        "start": "webpack-dev-server",
        "start:mock": "cross-env USE_MOCK=true NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
        "start:url": "cross-env URL=dev-espace.library.uq.edu.au NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server",
        "build:dev": "webpack --config=./webpack.config",
        "build": "webpack -p --config=./webpack-dist.config",
        "analyse": "webpack --config=./webpack-dist.config --profile --json > stats.json",
        "start:build": "cross-env NODE_ENV=production URL=dev-espace.library.uq.edu.au NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server --config=./webpack-dist.config --disable-host-check",
        "start:build:e2e": "cross-env NODE_ENV=production URL=localhost USE_MOCK=true NODE_OPTIONS='--max-old-space-size=2048' webpack-dev-server --config=./webpack-dist.config",
        "test": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest",
        "test:unit": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest test.js",
        "test:integration": "cross-env NODE_ENV=test FULL_PATH=http://localhost jest integration.js --no-coverage",
        "test:watch": "npm run test -- --watch --onlyChanged",
        "test:update": "npm run test -- --updateSnapshot",
        "test-unit": "node $NODE_DEBUG_OPTION ./node_modules/lab/bin/lab ./test/unit --pattern \"^[^_]\"",
        "eslint": "eslint '**/*.js' --quiet",
        "eslint:fix": "eslint '**/*.js' --quiet --fix",
        "codestyles:files": "prettier-eslint '**/*.js' --list-different -l silent",
        "codestyles:fix": "prettier-eslint --write",
        "codestyles:fix:all": "prettier-eslint '**/*.js' --write",
        "codestyles:fix:staged": "FILES=$(git diff --cached --name-only --diff-filter=ACM | grep \".jsx\\{0,1\\}$\"); npx prettier-eslint $FILES --write",
        "cypress:open": "cypress open",
        "cypress:watch": "cypress run --headed",
        "cypress:run": "cypress run",
        "e2e": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'cypress run'",
        "e2e:watch": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'cypress run --headed'",
        "e2e:open": "start-server-and-test 'npm run start:mock' http-get://localhost:3000 'cypress open'",
        "test:all": "npm run test && npm run e2e && npm run codestyles:files",
        "clean": "rm -rf assets coverage cypress/screenshots cypress/videos dist node_modules stats.json"
    },
    "engines": {
        "npm": "^6",
        "node": "11.10.1"
    },
    "babel": {
        "presets": [
            "@babel/env",
            "@babel/react"
        ],
        "env": {
            "development": {
                "presets": [
                    "@babel/env",
                    "@babel/react"
                ]
            },
            "production": {
                "presets": [
                    [
                        "@babel/env",
                        {
                            "es2015": {
                                "modules": false
                            },
                            "targets": {
                                "ie": 11,
                                "uglify": true
                            }
                        }
                    ]
                ],
                "plugins": [
                    "@babel/plugin-transform-react-constant-elements",
                    "transform-react-remove-prop-types",
                    "@babel/plugin-proposal-class-properties",
                    "@babel/plugin-proposal-export-namespace-from",
                    "@babel/plugin-proposal-export-default-from",
                    "@babel/plugin-proposal-class-properties",
                    [
                        "@babel/plugin-transform-spread",
                        {
                            "loose": true
                        }
                    ]
                ]
            },
            "test": {
                "presets": [
                    "@babel/env",
                    "@babel/react"
                ],
                "plugins": [
                    [
                        "dynamic-import-node",
                        {
                            "noInterop": true
                        }
                    ],
                    [
                        "@babel/plugin-transform-runtime",
                        {
                            "helpers": false,
                            "regenerator": true,
                            "corejs": false
                        }
                    ],
                    "@babel/plugin-proposal-export-namespace-from",
                    "@babel/plugin-proposal-export-default-from",
                    "@babel/plugin-proposal-class-properties"
                ]
            }
        }
    },
    "jest": {
        "transform": {
            ".*": "<rootDir>/node_modules/babel-jest"
        },
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(test).[jt]s?(x)",
            "**/?(*.)+(integration).[jt]s?(x)"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "es6"
        ],
        "unmockedModulePathPatterns": [
            "react",
            "enzyme"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/modules/**/*.{js,jsx}",
            "!src/modules/**/*.integration.{js,jsx}",
            "!src/**/index.js",
            "!src/**/containers/*.{js,jsx}",
            "src/modules/Index/*.{js,jsx}",
            "src/actions/*.{js,jsx}",
            "src/helpers/*.{js,jsx}",
            "src/middleware/*.{js,jsx}",
            "src/reducers/*.{js,jsx}",
            "src/repositories/routes.js"
        ],
        "coverageReporters": [
            "html",
            "text"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 100,
                "branches": 100,
                "functions": 100,
                "lines": 100
            }
        },
        "modulePaths": [
            "<rootDir>/src",
            "<rootDir>/eslint-plugin-uqlibrary",
            "<rootDir>/utils"
        ],
        "moduleNameMapper": {
            "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styles.js",
            "\\.(jpg|jpeg|svg)$": "<rootDir>/__mocks__/images.js"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "setupFiles": [
            "./src/test.setup.js",
            "jest-localstorage-mock"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/jest.setup.js"
        ],
        "testURL": "http://fez-staging.library.uq.edu.au"
    }
}
