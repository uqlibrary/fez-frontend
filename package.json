  "overrides": {
    "istanbul-lib-instrument": "6.0.1",
    "@istanbuljs/schema": "0.1.3",
    "istanbul-lib-coverage": "3.2.2"
  },
  "engines": {
    "npm": ">=10.8.2",
    "node": ">=20.11.0"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/preset-typescript"
    ],
    "env": {
      "development": {
        "presets": [
          "@babel/env",
          "@babel/react",
          "@babel/preset-typescript"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/env",
            {
              "modules": false,
              "targets": {
                "ie": 11
              }
            }
          ]
        ],
        "plugins": [
          "@babel/plugin-transform-react-constant-elements",
          "transform-react-remove-prop-types",
          "@babel/plugin-proposal-export-default-from",
          [
            "@babel/plugin-transform-spread",
            {
              "loose": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          "@babel/env",
          "@babel/react",
          "@babel/preset-typescript"
        ],
        "plugins": [
          [
            "dynamic-import-node",
            {
              "noInterop": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": false,
              "regenerator": true,
              "corejs": false
            }
          ],
          "@babel/plugin-proposal-export-default-from"
        ]
      },
      "cc": {
        "presets": [
          "@babel/env",
          "@babel/react",
          "@babel/preset-typescript"
        ],
        "plugins": [
          [
            "dynamic-import-node",
            {
              "noInterop": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": false,
              "regenerator": true,
              "corejs": false
            }
          ],
          "@babel/plugin-proposal-export-default-from",
          "istanbul"
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(test).[jt]s?(x)",
      "**/?(*.)+(integration).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/playwright/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "tsx",
      "ts",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/playwright/"
    ],
    "collectCoverageFrom": [
      "src/modules/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/modules/App/components/pages.js",
      "!src/**/{index,locale,config}.js",
      "!src/**/containers/*.{js,jsx,ts,tsx}",
      "!src/**/test-utils.{ts,js}",
      "src/modules/Index/*.{js,jsx,ts,tsx}",
      "src/actions/*.{js,jsx,ts,tsx}",
      "src/helpers/*.{js,jsx,ts,tsx}",
      "src/middleware/*.{js,jsx,ts,tsx}",
      "src/reducers/*.{js,jsx,ts,tsx}",
      "src/repositories/routes.js",
      "src/hooks/useForm.js",
      "src/hooks/useValidatedForm.js",
      "src/hooks/useDispatchOnce.ts",
      "src/hooks/usePrevious.ts",
      "src/hooks/useControlledVocabs.ts"
    ],
    "coverageDirectory": "coverage/jest",
    "coverageReporters": [
      "cobertura",
      "html",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/eslint-plugin-uqlibrary",
      "<rootDir>/utils"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styles.js",
      "\\.(jpg|jpeg|svg)$": "<rootDir>/__mocks__/images.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "reporters": [
      "default",
      "jest-summary-reporter",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report",
          "outputPath": "coverage/jest-html-report.html",
          "includeFailureMsg": true,
          "includeConsoleLog": true,
          "executionTimeWarningThreshold": 30,
          "statusIgnoreFilter": "passed"
        }
      ]
    ],
    "setupFiles": [
      "./src/test.setup.js",
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://fez-staging.library.uq.edu.au"
    },
    "testTimeout": 30000,
    "snapshotSerializers": [
      "<rootDir>/utils/jest-serializer.ts"
    ]
  },
  "nyc": {
    "reporter": [
      "html",
      "text-summary",
      "json"
    ],
    "temp-dir": "coverage/temp",
    "include": [
      "src/modules/**/*.{js,jsx,ts,tsx}",
      "src/modules/Index/*.{js,jsx,ts,tsx}",
      "src/actions/*.{js,jsx,ts,tsx}",
      "src/helpers/*.{js,jsx,ts,tsx}",
      "src/middleware/*.{js,jsx,ts,tsx}",
      "src/reducers/*.{js,jsx,ts,tsx}",
      "src/repositories/routes.js"
    ],
    "exclude": [
      "src/**/{index,locale,config,pages}.js",
      "src/**/containers/*.{js,jsx,ts,tsx}",
      "src/**/*.test.{js,jsx,ts,tsx}",
      "src/modules/Admin/hooks.js",
      "src/modules/SharedComponents/Toolbox/MenuDrawer/components/MenuDrawer.tsx"
    ],
    "all": true
  }
}